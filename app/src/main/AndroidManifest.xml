<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.destroy.ddopagoihanda" >

    <uses-permission android:name="android.permisson.CALL_PHONE"/>
    <uses-permission android:name="android.permisson.INTERNET"/>
    <uses-permission android:name="android.permisson.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permisson.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permisson.WRITE_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReadStudentActivity"
            android:label="@string/title_activity_read_student" >
        </activity>
        <activity
            android:name=".AddStudentActivity"
            android:label="@string/title_activity_add_student" >
        </activity>
    </application>

</manifest>
